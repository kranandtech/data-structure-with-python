    "#  Average in a list\n",
    "def avg(l):\n",
    "    sum = 0\n",
    "    for item in l:\n",
    "        sum+= item\n",
    "    n = len(l)\n",
    "    return sum/n;\n",
    "\n",
    "l = [3,6,7,8]\n",
    "print(avg(l))\n",
    "        \n",
    "    "
